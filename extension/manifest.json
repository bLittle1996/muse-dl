{
    "name": "muse-dl",
    "version": "1.0",
    "description": "Messing around with making chrome extensions",
    "manifest_version": 2,
    "permissions": [
        "storage"
    ],
    "background": {
        "scripts": [
            "scripts/background/index.js"
        ],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popups/default.html"
    }
}